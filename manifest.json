    "manifest_version": 3,
    "name": "Chrome To-Do",
    "description": "un Organizador de tareas de prueba. ",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/todolist.png",
        "32": "icons/todolist.png",
        "128": "icons/todolist.png"
    }
}